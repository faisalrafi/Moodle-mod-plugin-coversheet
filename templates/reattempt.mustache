{{#enable}}
        <div class="row">
            <div class="singlebutton quizstartbuttondiv">
                <form method="post" action="{{{formurl}}}">
                    <input type="hidden" name="id" value="{{cmid}}">
                    <input type="hidden" name="prev_attempt" value="{{prev_attempt}}">
                    <button type="submit" class="btn btn-primary mb-3 ml-3">Re-submission</button>
                </form>
            </div>
        </div>
{{/enable}}
{{^enable}}
    <div class="alert alert-danger alert-dismissible fade show" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        <h5>{{{attempt_text}}}</h5>
    </div>
{{/enable}}
{{#hasgrade}}
<div class="d-flex">
    <label>Grade:</label>
    {{#feedback_submit}}
        <p class="ml-2 font-weight-bold">{{grade}}</p>
    {{/feedback_submit}}
    {{^feedback_submit}}
        <p class="ml-2 font-weight-bold">Under Review</p>
    {{/feedback_submit}}
</div>
<div class="d-flex">
    <label>Comment:</label>
    <p class="ml-2 font-weight-bold">{{comment}}</p>
</div>
{{/hasgrade}}
<a href="{{webroot}}/mod/coversheet/adminpages/template_report.php?id={{cmid}}&studentid={{studentid}}" class=" btn btn-primary">View Coversheet</a>
<div class="py-3">
    <h4 class="report-heading">Last attempt information</h4>
    {{#datas}}
        <div class="d-flex">
            <label for="name">{{name}}:</label>
            <p class="ml-2">{{value}}</p>
        </div>
    {{/datas}}
</div>
<div class="">
    <h6 class="conclusion-heading">Name:</h6><p class="bold">{{sname}}</p>
    <h6 class="conclusion-heading">Signature:</h6>
    <img src="{{sign}}" alt="teacher_sign" height="200px" width="500px" class="conclusion-image">
</div>